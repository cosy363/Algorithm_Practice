array = [1, 3, 6, 7, 8, 10]

count = [0] * (max(array)+1)

for i in range(len(array)):
    count[array[i]] += 1 # index increase

result = []
for i in range(len(count)): # for length of data range
    for j in range(count[i]): # for number of data for corresponding index value
        result.append(i)

print(result)